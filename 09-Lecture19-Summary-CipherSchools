# print function
print("hello \"hello\' ")
escape sequence
print("line A \tlineB")
print("line B")
print("this is backslash \\\\")
escape sequence as normal text
# print(r"line A \n line B")
# python as calculator
print (2+3)
print(2-3)
print(2*3)
print(4/2)_ # float divison
print(4//2)_ # integer division
print(3//2)
print(2**3**2)
print((2+3) *4 / 2)
variables
name = “harshit"
print(name)
name = 123
print(name)

# naming rules for variables
# name = “harshit" #error
# namel = “harshit"#no error
# name$ = “harshit" #error
# $name = “harshit" #error
_name = “anyname” #no error


